#include <vector>
#include <string>
#include <fstream>
#include <iostream>
using namespace std;

//Array and for loops
int main(){

  int myFaveNums[10];

  int worstNums[10] = {1 ,2 ,3 ,4 ,5};

  cout << "Bad Number 1: " << worstNums[1] << endl;

  char myName[6][6] = {{'A','y','m','a','a','n'},{'S','h','a','i','k','h'}};

  cout << "3rd letter in 2nd array " << myName[3][1] << endl;

  myName[0][2] = 'e';

  cout << "New Value" << myName[0][2] << endl;

  for(int i = 1; i <=10; i++){
    cout << i << endl;
}
  for(int j = 0; j < 5; j++){
    for(int k = 0; k < 5; k++){

      cout << myName[j][k];

    }

    cout << endl;

  }
  return 0;


}
